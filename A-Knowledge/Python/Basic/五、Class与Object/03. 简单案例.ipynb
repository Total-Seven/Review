    "class Box():\n",
    "    def __init__(self,length1,width1,height1):\n",
    "        self.length = length1\n",
    "        self.width = width1\n",
    "        self.height = height1\n",
    "    \n",
    "    def volume(self):\n",
    "        return self.length * self.width * self.height\n",
    "\n",
    "my_box = Box(10,10,10)\n",
    "my_box.volume()"
   ]
