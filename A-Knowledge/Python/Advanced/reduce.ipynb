   "source": [
    "from functools import reduce\n",
    "\n",
    "# 基本用法\n",
    "# result = reduce(function, sequence, initial)\n",
    "\n",
    "# 示例\n",
    "numbers = [1, 2, 3, 4, 5]\n",
    "\n",
    "# 使用 reduce 对序列元素进行累积求和\n",
    "sum_result = reduce(lambda x, y: x + y, numbers)\n",
    "print(sum_result)  # 输出: 15\n"
   ]
  }
