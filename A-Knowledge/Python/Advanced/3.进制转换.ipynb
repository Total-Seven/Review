   "source": [
    "res = \"\"\n",
    "def fn(number, x):\n",
    "    global res\n",
    "    if number == 0:\n",
    "        return 0\n",
    "\n",
    "    res += str(number % x)\n",
    "\n",
    "    fn(number//x, x)\n",
    "\n",
    "fn(12,2)\n",
    "print(res[::-1])"
   ]
  }
